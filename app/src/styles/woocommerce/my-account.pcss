								font-weight: 500;
								line-height: 18.2px;
								letter-spacing: 0.28px;
								max-width: 108px;
								width: 100%;
								order: 4;

								@media (--md) {
									order: 3;
								}
							}

							.product-price {
								color: #000;
								font-family: $font-family-raleway;
								font-size: 14px;
								font-style: normal;
								font-weight: 500;
								line-height: 18.2px;
								letter-spacing: 0.28px;
								max-width: 186px;
								width: 100%;
								order: 3;

								@media (--md) {
									order: 4;
								}
							}

							.wc-item-meta {
								padding: 0;
								margin: 10px 0 0;

								li {
									color: #000;
									display: flex;
									margin: 10px 0;

									&:first-child {
										margin-top: 0;
									}

									&:last-child {
										margin-bottom: 0;
									}

									strong {
										display: block;
										margin-right: 5px;
									}
								}
							}

							.product-total {
								max-width: 255px;
								width: 100%;
								order: 5;

								@media (--md) {
									order: 5;
								}

								bdi {
									color: #000;
									font-family: $font-family-raleway;
									font-size: 14px;
									font-style: normal;
									font-weight: 600;
									line-height: 18.2px;
									letter-spacing: 0.28px;
								}
							}

							.product-add-cart {
								position: absolute;
								bottom: 17px;
								right: 20px;

								@media (--sm) {
									bottom: unset;
									position: absolute;
									top: 50%;
									transform: translateY(-50%);
									right: 0;
								}

								@media (--md) {
									order: 6;
									top: unset;
									transform: unset;
									right: unset;
									position: relative;
								}
							}
						}
					}

					tfoot {
						display: block;
						border: 1px solid #d9d9d9;
						width: 100%;
						max-width: 500px;
						margin-left: auto;

						tr {
							display: flex;
							justify-content: space-between;
							padding-bottom: 10px;
						}

						th {
							font-family: $font-family-raleway;
							color: #000;
							font-size: 16px;
							font-style: normal;
							font-weight: 400;
							line-height: 20.8px;
							letter-spacing: 0.32px;
							text-align: left;
							border-top: none;
						}

						td {
							color: #000;
							font-family: $font-family-raleway;
							font-size: 16px;
							font-style: normal;
							font-weight: 500;
							line-height: 20.8px;
							letter-spacing: 0.32px;
							display: block;
							text-align: right;

							span,
							small {
								color: #333;
							}
						}
					}
				}
			}
		}

		.woocommerce-customer-details {
			.col-1 {
				display: flex;
				flex-direction: column;
				gap: 30px;
			}

			.woocommerce-column {
				&__title {
					color: #000;
					font-family: $font-family-raleway;
					font-size: 24px;
					font-style: normal;
					font-weight: 500;
					line-height: 42px;
					letter-spacing: 0.56px;
				}
			}

			address {
				color: #000;
				font-family: $font-family-raleway;
				font-size: 16px;
				font-style: normal;
				font-weight: 400;
				line-height: normal;
				border-bottom-width: 1px;
				border-right-width: 1px;

				p {
					margin-bottom: 16px;
				}
			}
