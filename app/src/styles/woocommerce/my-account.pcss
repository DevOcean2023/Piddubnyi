								@media (--sm) {
									position: absolute;
									top: 0;
									left: 0;
									text-align: start;
								}

								@media (--md) {
									position: relative;
									top: unset;
									left: unset;
								}

								img {
									width: 100%;
									max-width: 150px;
									object-fit: cover;
								}
							}

							.product-name {
								width: 100%;
								max-width: 221px;
								min-width: 221px;
								order: 2;

								@media (--md) {
									order: 2;
								}

								a {
									width: 100%;
									display: block;
									font-family: $font-family-raleway;
									color: #000;
									font-size: 16px;
									font-style: normal;
									font-weight: 400;
									line-height: 24px;
									text-align: center;

									@media (--sm) {
										text-align: start;
									}

									@media (--md) {
										text-align: start;
									}
								}
							}

							.product-quantity {
								color: #000;
								font-family: $font-family-raleway;
								font-size: 14px;
