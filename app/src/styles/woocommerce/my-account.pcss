	}

	.woocommerce {
		display: flex;
		flex-direction: column;
	}

	.woocommerce-notices-wrapper {
		position: relative;
		background: transparent;
		width: 100%;
		max-width: 100%;
		z-index: 4;
		margin: 0 0 16px;

		.woocommerce-message {
			border-top-color: #8fae1b;
			background: #fff;
			color: #000;
			font-family: $font-family-raleway;
			font-size: 16px;
			font-style: normal;
			font-weight: 400;
			line-height: 24px;

			+ .woocommerce-message {
				display: none;
			}
		}

		p {
			color: #000;
			font-family: $font-family-raleway;
			font-size: 16px;
			font-style: normal;
			font-weight: 400;
			line-height: 24px;

			strong {
				font-weight: 600;
			}

			a {
				font-weight: 600;
				color: #015f45;
			}
		}
	}

	.woocommerce-form-row {
		&_in-focus-or-has-value {
			label {
				top: 0;
				left: 10px;
				font-size: 11px;
			}
		}
	}

	.woocommerce-pagination {
		.woocommerce-button {
			display: block;
			margin: 20px auto;
			border: 1px solid #000;
			background: #000;
			color: #fff;

			&:hover {
				background: transparent;
				color: #000;
				box-shadow: none;
			}
		}
	}
}

.my-edit-address-class {
	.default-page {
		[class*="woocommerce-MyAccount-content"] {

